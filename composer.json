{
  "name": "wizhippo/deferred-visibility-bundle",
  "description": "Deferred Visibility Bundle is an eZ Platform bundle to defer content visibility after publishing and hide at a later date.",
  "license": "GPL-2.0",
  "type": "ezplatform-bundle",
  "authors": [
    {
      "name": "Douglas Hammond",
      "email": "wizhippo@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "ezsystems/ezpublish-kernel": "^6.5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Wizhippo\\Bundle\\DeferredVisibilityBundle\\": "bundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wizhippo\\Bundle\\DeferredVisibilityBundle\\": "tests/"
    },
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  }
}
