{
  "name": "wizhippo/ezplatform-deferred-visibility-bundle",
  "description": "Deferred Visibility Bundle is an eZ Platform bundle to defer content visibility after publishing and hide at a later date.",
  "license": "GPL-2.0",
  "type": "ezplatform-bundle",
  "authors": [
    {
      "name": "Douglas Hammond",
      "email": "wizhippo@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ezsystems/ezplatform-kernel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0.0"
  },
  "conflict": {
    "symfony/cache": "5.1.3"
  },
  "autoload": {
    "psr-4": {
      "Wizhippo\\WizhippoDeferredVisibilityBundle\\": "bundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wizhippo\\WizhippoDeferredVisibilityBundle\\Tests\\": "tests/"
    }
  }
}
